        <Label Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center" Width="320" Content="U IZRADI!" FontSize="72"/>
        <Button Width="200" Height="80" Margin="0,200,0,0" Background="#FF215913" Content="Odjavi se" FontSize="32" Click="Button_Click" materialDesign:ButtonAssist.CornerRadius="5"/>
    </Grid>
